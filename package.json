{
  "name": "v-show-lazy",
  "version": "0.0.0",
  "description": "v-show-lazy for vue",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdzhaoxin/v-show-lazy.git"
  },
  "author": "Lakindu Hewawasam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdzhaoxin/v-show-lazy/issues"
  },
  "homepage": "https://github.com/rdzhaoxin/v-show-lazy",
  "main": "./dist/v-show-lazy.cjs",
  "module": "./dist/v-show-lazy.js",
  "exports": {
    ".": {
      "import": "./dist/v-show-lazy.js",
      "require": "./dist/v-show-lazy.cjs"
    },
    "./secondary": {
      "import": "./dist/v-show-lazy.js",
      "require": "./dist/v-show-lazy.cjs"
    }
  },
  "typings": "./dist/types/global.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "vite",
    "dts": "vue-tsc src/index.ts --declaration --noEmit false --emitDeclarationOnly --outFile dist/types/global.d.ts",
    "build": "vue-tsc && vite build && nr dts",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  }
}
